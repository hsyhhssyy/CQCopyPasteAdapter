<UserControl x:Class="CQCopyPasteAdapter.ViewModel.SettingsManageControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CQCopyPasteAdapter.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DataGrid Name="dgrSettings" AutoGenerateColumns="False" CanUserAddRows="False">
        <DataGrid.Columns>
            <DataGridTextColumn Header="Key" Binding="{Binding Key}" IsReadOnly="True"/>
            <DataGridTextColumn Header="Value" Binding="{Binding Value}"/>
        </DataGrid.Columns>
    </DataGrid>
</UserControl>
