<Window x:Class="CQCopyPasteAdapter.Dialogs.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CQCopyPasteAdapter.Dialogs"
        mc:Ignorable="d"
        Title="InputBox" Height="150" Width="300">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Enter a string:"/>
            <TextBox x:Name="inputTextBox"/>
            <Button Content="OK" IsDefault="True" Click="OkButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
